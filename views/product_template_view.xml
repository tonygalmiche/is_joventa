        <!--product.template --> 
        <record model="ir.ui.view" id="is_view_product_template_form">
            <field name="name">is_view_product_template_form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <field name="list_price" position="after">
                    <field name="is_stock_mini"/>
                    <field name="is_famille_article"/>
                    <field name="is_dn_article"/>
                    <field name="is_diaphragme"/>
                    <field name="is_contacts_auxiliaires"/>
                    <field name="is_type_vanne_article"/>
                    <field name="is_couple_article"/>
                    <field name="is_motorisation_article"/>
                    <field name="is_signalcommande_article"/>
                    <field name="is_tension_article"/>
                </field>
                <field name="categ_id" position="after">
                    <field name="is_code_douanier"/>
                    <field name="is_origine_article"/>
                </field>
            </field>
        </record>  
        <record id="is_action_product_form" model="ir.actions.act_window">
            <field name="name">Articles</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form</field>
            <field name="view_id" ref="product.product_template_tree_view"/>
            <field name="context">{"search_default_filter_to_sell":1}</field>
        </record>
