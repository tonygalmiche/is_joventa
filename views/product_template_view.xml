<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <!-- is.couple.article -->
        <record model="ir.ui.view" id="is_couple_article_form_view">
            <field name="name">is_couple_article_form_view</field>
            <field name="model">is.couple.article</field>
            <field name="arch" type="xml">     
                <form string="couple article" version="7.0">  
                    <sheet>
                        <group>
                            <field name="name"/> 
                            <field name="commentaire"/> 
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
       <record model="ir.ui.view" id="is_couple_article_tree_view">
            <field name="name">is_couple_article_tree_view</field>
            <field name="model">is.couple.article</field>
            <field name="arch" type="xml">
                <tree string="couple article">
                    <field name="name"/> 
                    <field name="commentaire"/> 
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="is_couple_article_filter_view" >
            <field name="name">is_couple_article_filter_view</field>
            <field name="model">is.couple.article</field>
            <field name="arch" type="xml">
                <search string="Recherche">
                    <field name="name"        filter_domain="[('name'       , 'ilike', self)]"/>
                    <field name="commentaire" filter_domain="[('commentaire', 'ilike', self)]"/>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="is_couple_article_action">
            <field name="name">Couple article</field>
            <field name="res_model">is.couple.article</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>


